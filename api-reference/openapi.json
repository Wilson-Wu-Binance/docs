					{
						"name": "X-API-Key",
						"in": "header",
						"description": "Your HashDit API key",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"requestBody": {
					"description": "Blockchain address to classify",
					"required": true,
					"content": {
